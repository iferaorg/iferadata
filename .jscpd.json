{
  "threshold": 0,
  "reporters": ["html", "markdown"],
  "ignore": [
    "**/node_modules/**",
    "**/.git/**",
    "**/.rbenv/**",
    "**/.venv/**",
    "**/report/**",
    "**/megalinter-reports/**",
    "**/hardis-report/**",
    "**/*cache*/**",
    "**/*.json",
    "**/*.yaml",
    "**/*.yml",
    "**/*.md",
    "**/*.html",
    "**/*.xml",
    "**/*.jpg",
    "**/*.png",
    "**/*.svg",
    "**/*.zip",
    "**/*.bin"
  ],
  "ignorePattern": [
    "def\\s+\\w+\\s*\\([\\s\\S]*?\\)\\s*(->[\\s\\S]*?)?:"
  ]
}
